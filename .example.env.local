NEXT_PUBLIC_SUPABASE_URL=YOUR_SUPABASE_URL
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Site URL for redirects (change to your domain in production)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Required for the voucher claiming API route.
# Get this from your Supabase project settings: Project Settings > API > Service Role Key
# WARNING: Keep this key secret and never expose it client-side!
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY